{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "skipLibCheck": false,
    "incremental": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "importHelpers": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowJs": true,
    "rootDirs": [
      "src",
      "./__tests__/"
    ],
    "outDir": "out"
  }
}