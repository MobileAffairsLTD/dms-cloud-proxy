{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "skipLibCheck": false,
    "incremental": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "importHelpers": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowJs": false,
    "rootDirs": [
      "src",
      "./__tests__/"
    ],
    "baseUrl": "./",
    "paths": {
      "tslib": ["node_modules/tslib"],
    },
    "lib": [
      "es2015",
    ],
    "outDir": "./out"
  },
  "exclude": [
    "../../node_modules",
    "node_modules",
    //"__tests__",
    "lib",
    ".tmp"
  ]
}